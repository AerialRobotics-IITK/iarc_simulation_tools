            <child>palm</child>
            <axis>
                <limit>
                    <lower>0</lower>
                    <upper>0.9</upper>
                    <!-- <velocity>0.01</velocity> -->
                </limit>
                <xyz>0 0 1</xyz>
            </axis>
        </joint>


        <plugin name="grabber_plugin" filename="libiarc_grabber_plugin.so">
            <namespace>firefly</namespace>
            <link_name1>right_finger</link_name1>
            <link_name2>left_finger</link_name2>
            <force_magnitude>10</force_magnitude>
