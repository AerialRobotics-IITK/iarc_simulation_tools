            <parent>right_finger</parent>
            <child>palm</child>
            <axis>
                <limit>
                    <lower>0</lower>
                    <upper>0.04</upper>
                    <velocity>0.01</velocity>
                </limit>
                <xyz>0 -1 0</xyz>
            </axis>
        </joint>
        <joint name="left_hand" type="prismatic">
            <parent>left_finger</parent>
            <child>palm</child>
