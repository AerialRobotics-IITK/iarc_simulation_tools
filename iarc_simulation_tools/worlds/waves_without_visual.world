  <model name="1_sphere">
    <pose>0.4 0 10 0 0 0</pose>
    <static>False</static>
    <link name="link">
      <visual name="visual">
        <geometry>
          <sphere>
            <radius>0.106</radius>
          </sphere>
        </geometry>
      </visual>
      <collision name="collision">
        <geometry>
          <sphere>
            <radius>0.106</radius>
          </sphere>
        </geometry>
      </collision>
      <inertial>
        <mass>2</mass>
        <inertia>
          <ixx>0.0089888</ixx>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyy>0.0089888</iyy>
          <iyz>0</iyz>
