  </model>
  <model name="1_cylinder">
    <pose>0.7 0 10 0 0 0</pose>
    <static>False</static>
    <link name="link">
      <visual name="visual">
        <geometry>
          <cylinder>
            <radius>0.1</radius>
            <length>0.159</length>
          </cylinder>
        </geometry>
      </visual>
      <collision name="collision">
        <geometry>
          <cylinder>
            <radius>0.1</radius>
            <length>0.159</length>
          </cylinder>
        </geometry>
      </collision>
      <inertial>
        <mass>2</mass>
        <inertia>
          <ixx>0.0092134963146</ixx>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyy>0.0092134963146</iyy>
          <iyz>0</iyz>
          <izz>0.01</izz>
        </inertia>
      </inertial>
    </link>
    <!-- <plugin filename="libbuoyancy_gazebo_plugin.so" name="BuoyancyPlugin">
        <fluid_density>1000</fluid_density>
        <fluid_level>0.0</fluid_level>
        <linear_drag>10.0</linear_drag>
